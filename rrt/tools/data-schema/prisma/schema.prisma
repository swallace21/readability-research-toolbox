// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  created DateTime
}

model Study {
  id    Int    @id @default(autoincrement())
  name  String
  start DateTime
  end DateTime
}

model Design {
  id    Int    @id @default(autoincrement())
  name  String @unique
  design  Json
}
